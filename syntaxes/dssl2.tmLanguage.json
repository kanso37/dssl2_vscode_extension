{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "dssl2",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#variables"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
				"name": "keyword.control.dssl2",
				"match": "\\b(def|class|assert|test|assert_error|return|pass|struct|interface|assert_eq)\\b"
				},
				{
				"name": "keyword.other.dssl2",
				"match": "\\b(placeholder)\\b"
				}
			]
			},
		"comments": {
			"patterns": [
				{
				"name": "comment.line.octothorp.dssl2",
				"begin": "#",
				"end": "\n"

				}
			]
		},
		"strings":{
			"patterns": [
				{
				"name":"string.quoted.single.dssl2",
				"begin": "'",
				"end": "'"
				},
				{
				"name":"string.quoted.double.dssl2",
				"begin": "\"",
				"end": "\""
				}
			]
		},
		"constants": {
			"patterns": [
				{
				"name": "constant.character",
				"match": "(<|>|!=|=|==|-|%)"
				},
				{
				"name": "constant.language",
				"match": "\\b(orange)\\b"
				},
				{
					"name": "constant.numericfloat",
					"match": "(?x)\n  (?<! \\w)(?:\n    (?:\n      \\.[0-9](?: _?[0-9] )*\n      |\n      [0-9](?: _?[0-9] )* \\. [0-9](?: _?[0-9] )*\n      |\n      [0-9](?: _?[0-9] )* \\.\n    ) (?: [eE][+-]?[0-9](?: _?[0-9] )* )?\n    |\n    [0-9](?: _?[0-9] )* (?: [eE][+-]?[0-9](?: _?[0-9] )* )\n  )([jJ])?\\b\n"
				},
				{
				"name": "constant.numericdec",
				"match": "(?x)\n  (?<![\\w\\.])(?:\n      [1-9](?: _?[0-9] )*\n      |\n      0+\n      |\n      [0-9](?: _?[0-9] )* ([jJ])\n      |\n      0 ([0-9]+)(?![eE\\.])\n  )\\b\n"
				}
			]
		},
		"variables":{
			"patterns": [
				{
				"name": "variable.language.dssl2"	,
				"match": "\\b(let|and|or|if|elif|else|not|for|in|is|False|True|None)\\b"
				},
				{
				"name": "variable.userdefined.dssl2",
				"match": "[^,.():;<>=\\[\\]]"
				}
			]
		}
	},
	"scopeName": "source.rkt"
}